<!DOCTYPE html>
<html lang="en">
    <head>
        <title>List User</title>
        <meta charset="UTF-8">
        <meta name="viewport" content="width=device-width, initial-scale=1">
    </head>
    <body>
        <table>
            <thead>
                <tr>
                    <th>Email</th>
                    <th>PassWord</th>
                    <th>Name</th>
                    <th>Phone</th>
                </tr>
            </thead>
            <tbody>
                <%array.forEach(function(user) {%>
                    <tr>
                        <td><%= user.email%></td>
                        <td><%= user.password%></td>
                        <td><%= user.name%></td>
                        <td><%= user.phone%></td>
                    </tr>
                <%}, this);%>
                
            </tbody>
        </table>
    </body>
</html>